% Custom commands
\newcommand{\resumeItem}[1]{
  \item\small{
    {#1 \vspace{-2pt}}
  }
}

\newcommand{\classesList}[4]{
    \item\small{
        {#1 #2 #3 #4 \vspace{-2pt}}
  }
}

\newcommand{\resumeSubheading}[4]{
  \vspace{-2pt}\item
    \begin{tabular*}{1.0\textwidth}[t]{l@{\extracolsep{\fill}}r}
      \textbf{#1} & \textbf{\small #2} \\
      \textit{\small#3} & \textit{\small #4} \\
    \end{tabular*}\vspace{-7pt}
}
\newcommand{\resumeSubheadingNormal}[2]{
  \vspace{-2pt}\item
    \begin{tabular*}{1.0\textwidth}[t]{l@{\extracolsep{\fill}}r}
      \small #1 & \textbf{\small #2} \\
    \end{tabular*}\vspace{-3pt}
}

\newcommand{\resumeSubSubheading}[2]{
    \item
    \begin{tabular*}{0.97\textwidth}{l@{\extracolsep{\fill}}r}
      \textit{\small#1} & \textit{\small #2} \\
    \end{tabular*}\vspace{-7pt}
}

\newcommand{\resumeProjectHeading}[2]{
    \item
    \begin{tabular*}{1.001\textwidth}{l@{\extracolsep{\fill}}r}
      \small#1 & \textbf{\small #2}\\
    \end{tabular*}\vspace{-7pt}
}

\newcommand{\resumeSubItem}[1]{\resumeItem{#1}\vspace{-4pt}}

\renewcommand\labelitemi{$\vcenter{\hbox{\tiny$\bullet$}}$}
\renewcommand\labelitemii{$\vcenter{\hbox{\tiny$\bullet$}}$}

\newcommand{\resumeSubHeadingListStart}{\begin{itemize}[leftmargin=0.0in, label={}]}
\newcommand{\resumeSubHeadingListEnd}{\end{itemize}}
\newcommand{\resumeItemListStart}{\begin{itemize}}
\newcommand{\resumeItemListEnd}{\end{itemize}\vspace{-5pt}}
\newcommand{\resumeSubSubheadingLeft}[2]{ % added
\begin{description}[leftmargin=!,labelwidth=\widthof{\small\bfseries #1}]
    \item[\small #1]{\textit{\small  #2}}
\end{description}
}

\begin{document}
\begin{center}
  {\Huge \scshape {{ name }}} \\ \vspace{1pt}
  % 123 Street Name, Town, State 12345 \\ \vspace{1pt}
  \small \raisebox{-0.1\height}
  \faPhone\ {{ phone }}
  ~ \href{mailto:{{ email }}}{\raisebox{-0.2\height} \faEnvelope\ \underline{ {{ email }} }}
  ~ \href{https://{{ website }}}{\raisebox{-0.2\height} \faHome\ \underline{ {{ website }} }}
  ~ \href{https://linkedin.com/in/{{ linkedin }}}{\raisebox{-0.2\height} \faLinkedin\ \underline{in/{{ linkedin }}}}
  ~ \href{https://github.com/{{ github }}}{\raisebox{-0.2\height} \faGithub\ \underline{ {{ github }} }}
  \vspace{-8pt}
\end{center}


%-----------EDUCATION-----------
\section{Education}
  \resumeSubHeadingListStart
  {% for education in education_list %}
    \resumeSubheading
      { {{ education.institution }} }{ {{ education.graduation }} }
      { {{ education.degree }} } { {{ education.extra_info}} }
  {% endfor %}
  \resumeSubHeadingListEnd

%-----------TECHNICAL SKILLS-----------
\section{Technical Skills}
 \begin{itemize}[leftmargin=0.15in, label={}]
    \small{\item{
      {% for skill in skills %}
        \textbf{ {{skill.section_name}} }{: {{ skill.details | join(", ") }}} \\
      {% endfor %}
    }}
 \end{itemize}
 \vspace{-16pt}

%-----------EXPERIENCE-----------
\section{Experience}
  \resumeSubHeadingListStart
  {% for experience in experience_list %}
    \resumeSubheading
      { {{ experience.company }} }{ {{ experience.duration }} }
      { {{ experience.role }} }{ {{ experience.location }} }
      \resumeItemListStart
      {% for item in experience.details %}
        \resumeItem{ {{ item }} }
      {% endfor %}
      \resumeItemListEnd
  {% endfor %}
  \resumeSubHeadingListEnd
\vspace{-16pt}

%-----------PROJECTS-----------
\section{Technical Projects}
    \vspace{-5pt}
    \resumeSubHeadingListStart
    {% for project in project_list %}
      \resumeProjectHeading
      {\href{ {{ project.link }} }{\textbf{ {{- project.name -}} }} $|$ \emph{ {{ project.description }} }}{ {{ project.duration }} }
      \resumeItemListStart
      {% for item in project.details %}
        \resumeItem{ {{ item }} }
      {% endfor %}
      \resumeItemListEnd
      {% if not loop.last %}
        \vspace{-13pt}
      {% endif %}
    {% endfor %}
    \resumeSubHeadingListEnd
\vspace{-15pt}

%-----------AWARDS---------------
\section{Awards}
    \resumeSubHeadingListStart
    {% for award in awards_list %}
        \resumeSubheadingNormal{\textbf{ {{ award.name }} } | \textit{ {{ award.description }} }}{ {{ award.date }} }
        \vspace{-16pt}
    {% endfor %}
    \resumeSubHeadingListEnd

